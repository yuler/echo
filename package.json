{
  "name": "reads",
  "description": "This package.json only use npm scripts alias some commands for quickly running.",
  "packageManager": "pnpm@10.10.0",
  "private": true,
  "scripts": {
    "dev": "./bin/dev",
    "test": "./bin/rails test",
    "lint:fix": "./bin/rubocop -f github -a",
    "open:dev:db": "open storage/development.sqlite3",
    "app:deploy": "./bin/kamal deploy",
    "app:shell": "./bin/kamal shell",
    "app:console": "./bin/kamal console",
    "app:logs": "./bin/kamal logs",
    "backup:download": "./bin/rake backup:download",
    "backup:import": "./bin/rake backup:import"
  }
}